{
  "env": {
    "browser": true,
    "es6": false,
    "jquery": true
  },
  "extends": [
    "plugin:@wordpress/eslint-plugin/es5"
    /*"eslint:recommended",
    "plugin:es5/no-es2015",
    "plugin:es5/no-es2016"*/
  ],

  "rules": {
    "comma-dangle": ["error", "never"]
  },
  "parserOptions": {
    "ecmaVersion": 5
  },
  "plugins": [
    "es5"
  ]
}